{
  "name": "frontend",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "devDependencies": {
    "husky": "^9.1.7"
  },
  "scripts": {
    "prepare": "cd .. && husky frontend/.husky",
    "lint-staged": "yarn workspaces foreach run lint-staged",
    "start:web": "yarn --cwd apps/web start",
    "build:web": "yarn --cwd apps/web build"
  },
  "packageManager": "yarn@4.9.2"
}
